{
    "name": "ngx-size-me",
    "private": false,
    "version": "8.0.0",
    "description": "Makes your angular components aware of width, height and position change",
    "main": "./bundles/ngx-size-me.umd.js",
    "module": "./fesm5/ngx-size-me.js",
    "es2015": "./fesm2015/ngx-size-me.js",
